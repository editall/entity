{
  "name": "@entityjs/entityjs",
  "version": "0.0.6",
  "description": "entity framework for pure javascript",
  "type": "module",
  "main": "./src/index.js",
  "module": "./dist/entityjs.es.js",
  "directories": {
    ".": "./src"
