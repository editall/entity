  "contributors": [
    {
      "name": "hikaMaeng"
    },
    {
      "name": "easylogic",
      "email": "cyberuls@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/entityjs/entityJS/issues"
  },
  "homepage": "https://github.com/entityjs/entityJS#readme",
  "devDependencies": {
    "release-it": "^14.11.8",
    "vite": "^2.7.10",
    "vitest": "0.0.134"
